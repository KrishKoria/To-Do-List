<%- include('header'); -%>
		<div class="box" id="heading">
			<h1><%= listTitle %></h1>
		</div>
		<div class="box">
			<% for(let item of newListItems){%>
                <div class="item">
					<label for="CheckBox"><input type="checkbox"></label>
					<p><%= item %></p>
                </div>
			<% } %>
            <form class="item" action="/" method="post">
				<label for="input Field"><input type="text" name="newItem"  placeholder="New Item" autocomplete="off"/></label>
				<button type="submit" name="list" value = <%= listTitle %> >+</button>
		</form>
        </div>
<%- include('footer'); -%>
